{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AdminSettings": {
    "AdminToken": "admin-token"
  },
  "JwtSettings": {
    "SecretKey": "secret_key", // Use a strong key
    "Issuer": "your_issuer",
    "Audience": "your_audience"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=app.db"
  }

}
